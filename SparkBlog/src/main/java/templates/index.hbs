<!DOCTYPE html>
<html>
{{#partial "content"}}
    <body>
        <section>
            <div class="container">
                <div class="entry-list">
               	{{#each entries}}
					<article>
                        <h2><a href="/detail/{{ slug }}">{{ title }}</a></h2>
                        <time>{{dateTime}}</time>
                    </article>
        		{{/each}} 
                </div>
            </div>
        </section>
    </body>
    {{/partial}}
</html>
{{> base.hbs}}